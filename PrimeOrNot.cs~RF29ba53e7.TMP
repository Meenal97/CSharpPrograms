using System;
using System.Collections.Generic;
using System.Text;

namespace CSharpProgramms
{
    class PrimeOrNot
    {
        private int number;
        bool prime;

        public int Number
        {
            set
            {
                this.number = value;
            }
            get
            {
                return number;
            }
        }

        public bool Prime(int number)
        {
            for(int i=2;i<number/2;i++)
            {
                if(number % i == 0)
                {
                    prime = true;
                    break;
                }
            }
            return prime == true ? true : false;
        }
        
    }
}
